<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <com.rgsc.myapplication.TextView
        app:mytext="辽宁yjh"
        app:mytextColor="@color/blue"
        app:myinputType="number"
        app:mytextSize="20sp"
        android:padding="10dp"
        android:background="@color/design_default_color_secondary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.rgsc.myapplication.QQStepView
        android:id="@+id/stepview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:innerColor="@color/blue"
        app:stepTextColor="@color/black"
        app:borerWidth="50dp"
        app:stepTextSize="60sp"
        app:outerColor="@color/pink"/>
</LinearLayout>